<pretty-config xmlns="http://ocpsoft.org/prettyfaces/3.3.3" 
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
               xsi:schemaLocation="http://ocpsoft.org/prettyfaces/3.3.3               http://ocpsoft.org/xml/ns/prettyfaces/ocpsoft-pretty-faces-3.3.3.xsd">

    <url-mapping id="tournaments">
        <pattern value="/tournaments" />
        <view-id value="/pages/tournaments.xhtml" />
    </url-mapping>
    
    <url-mapping id="tournamentsDetails">
        <pattern value="/tournaments/#{id}" />
        <view-id value="/pages/tournaments.xhtml" />
    </url-mapping>
    
    <url-mapping id="editProfile">
        <pattern value="/edit" />
        <view-id value="/userPages/editProfile.xhtml" />
    </url-mapping>

</pretty-config>